{
  "name": "duet-night-abyss",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:studio": "next dev --turbopack",
    "build": "npm run generate:search-data && next build",
    "build:static": "npm run generate:search-data && STATIC_EXPORT=true next build",
    "build:turbo": "npm run generate:search-data && next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "studio": "sanity dev",
    "setup:sanity": "ts-node scripts/setup-sanity.ts",
    "quick-start": "ts-node scripts/quick-start.ts",
    "import:characters": "ts-node scripts/importCharacters.ts",
    "import:weapons": "ts-node scripts/importWeapons.ts",
    "convert:data": "ts-node scripts/convertExistingData.ts",
    "check:characters": "ts-node scripts/checkSanityCharacters.ts",
    "debug:characters": "ts-node scripts/debugSanityCharacters.ts",
    "check:character-names": "ts-node scripts/checkInvalidCharacterNames.ts",
    "find:problematic-character": "ts-node scripts/findProblematicCharacter.ts",
    "check:all-datasets": "ts-node scripts/checkAllDatasets.ts",
    "delete:problematic-characters": "ts-node scripts/deleteProblematicCharacters.ts",
    "delete:problematic-characters:execute": "ts-node scripts/deleteProblematicCharacters.ts --execute",
    "clear:sanity": "ts-node scripts/clearAllSanityData.ts",
    "generate:search-data": "ts-node scripts/generateSearchData.ts"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@sanity/vision": "^4.10.2",
    "groq": "^4.10.2",
    "next": "15.5.4",
    "next-sanity": "^11.4.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "sanity": "^4.10.2",
    "sharp": "^0.34.4",
    "styled-components": "^6.1.19",
    "tailwindcss": "^3.4.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@sanity/client": "^6.29.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "dotenv": "^16.3.1",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
